    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "prettier": "^2.0.2",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0"
